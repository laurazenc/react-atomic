{
  "name": "react-layouts",
  "version": "0.0.1",
  "description": "Layouts and components using style-components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "copy": "rm -rf dist/cjs/* && rm -rf dist/umd/* && cp README.md dist",
    "build": "rollup -c",
    "rollup": "rollup -c -w",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "typecheck": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurazenc/react-layouts.git"
  },
  "keywords": [
    "react",
    "styled-components",
    "atomic design"
  ],
  "author": "laurazenc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/laurazenc/react-layouts/issues"
  },
  "homepage": "https://github.com/laurazenc/react-layouts#readme",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.7.4",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.30.1",
    "rollup-plugin-terser": "^5.2.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/setupFile.js"
    ]
  }
}
